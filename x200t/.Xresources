!  Font
URxvt*font: xft:Determination Mono:style=Regular:size=10
URxvt*boldFont: xft:Determination Mono:style=Regular:size=10

URxvt.boldMode: false
URxvt*colorBDMode: true

Xft.dpi:	90
Xft.antialias:	false

!  Xft.rgba:	rgb
!  Xft.hinting:	true
!  Xft.hintstyle:	hintslight
!  Xft.autohint:	false
!  Xft.lcdfilter:	lcddefault

!  URxvt Resize Font
!  URxvt.perl-ext-common:	 resize-font
!  URxvt.keysym.C-minus:	 resize-font:smaller
!  URxvt.keysym.C-plus:	 resize-font:bigger
!  URxvt.keysym.C-equal:	 resize-font:reset
!  URxvt.keysym.C-question: resize-font:show

URxvt.scrollBar: false

!  urxvt*transparent: true
!  urxvt*shading: 50
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0000/8000

!  Every shell is a login shell
xterm*loginShell: true

xterm*vt100*geometry: 80x50

!  Scrollback
xterm*saveLines: 2000
URxvt.saveLines: 16834

xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48

xterm*termName: xterm-color
xterm*eightBitInput: false

!  Colors
!  Text (Normal, Normal, Cursor)
!  Black (Cursor)
!  Red (Tag)
!  Green (Special Key)
!  Yellow (Keyword)
!  Blue (Number)
!  Magenta (Precondit)
!  Cyan (Float)
!  White (Search)
!  FMT(?) (Include, StatusLine, ErrorMsg)

! special
*.foreground:   #b8b8b8
*.background:   #000000
*.cursorColor:  #b8b8b8

! black
*.color0:       #000000
*.color8:       #000000

! red
*.color1:       #575757
*.color9:       #8b2749

! green
*.color2:       #57c489
*.color10:      #87bda0

! yellow
*.color3:       #10bccd
*.color11:      #609fa5

! blue
*.color4:       #102973
*.color12:      #46588b

! magenta
*.color5:       #ea336f
*.color13:      #f27e94

! cyan
*.color6:       #b51166
*.color14:      #ba4980

! white
*.color7:       #ffffff
*.color15:      #ffffff
