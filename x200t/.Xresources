!  Font

URxvt*font: xft:Misc Tamsyn:size=13:style=Bold
URxvt*boldFont: xft:Misc Tamsyn:size=13:style=Bold
!URxvt*font: xft:Determination Mono:style=Regular:size=10
!URxvt*boldFont: xft:Determination Mono:style=Regular:size=10

URxvt.boldMode: false
URxvt*colorBDMode: true
!URxvt*letterSpace:	-1

Xft.dpi:	75
Xft.antialias:	true

!  Xft.rgba:	rgb
!  Xft.hinting:	true
!  Xft.hintstyle:	hintslight
!  Xft.autohint:	false !  Xft.lcdfilter:	lcddefault

!  URxvt Resize Font
!  URxvt.perl-ext-common:	 resize-font
!  URxvt.keysym.C-minus:	 resize-font:smaller
!  URxvt.keysym.C-plus:	 resize-font:bigger
!  URxvt.keysym.C-equal:	 resize-font:reset
!  URxvt.keysym.C-question: resize-font:show

URxvt.scrollBar: false

!  urxvt*transparent: true
!  urxvt*shading: 50
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0000/AF00
!URxvt*background: rgba:0500/0000/1500/D000
!URxvt*background: rgba:0800/0000/1F00/BD00

!  Every shell is a login shell
xterm*loginShell: true

xterm*vt100*geometry: 80x50

!  Scrollback
xterm*saveLines: 2000
URxvt.saveLines: 16834

xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48

xterm*termName: xterm-color
xterm*eightBitInput: false

!  Colors
!  Text (Normal, Normal, Cursor)
!  Black (Cursor)
!  Red (Tag)
!  Green (Special Key)
!  Yellow (Keyword)
!  Blue (Number)
!  Magenta (Precondit)
!  Cyan (Float)
!  White (Search)
!  FMT(?) (Include, StatusLine, ErrorMsg)

